# Stagehand Environment (LOCAL or BROWSERBASE)
STAGEHAND_ENV=LOCAL

# Airbnb Credentials
AIRBNB_EMAIL=your_email@example.com
AIRBNB_PASSWORD=your_password

# AADE Credentials
AADE_USERNAME=your_taxisnet_username
AADE_PASSWORD=your_taxisnet_password

# Browserbase (only needed if STAGEHAND_ENV=BROWSERBASE)
BROWSERBASE_API_KEY=your_api_key
BROWSERBASE_PROJECT_ID=your_project_id

# Safety Mode (set to "false" to actually submit to AADE)
DRY_RUN=true
